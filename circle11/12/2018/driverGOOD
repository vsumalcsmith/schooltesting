    System.out.println("1.\nNumber of Objects:"+Circle.getNumberOfObjects());
   
    Circle circle3 = new Circle(10);
    System.out.println("1.\nNumber of Objects:"+Circle.getNumberOfObjects());
  
    
    
    //Display circle
    System.out.println( "circle is:");
    System.out.println(circle1);
    
